      "delete": {
        "tags": [
          "slurm"
        ],
        "operationId": "cancel_job",
        "summary": "cancel or signal job",
        "parameters": [
          {
            "name": "job_id",
            "in": "path",
            "description": "Slurm Job ID",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "signal",
            "in": "query",
            "description": "signal to send to job",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "description": "POSIX signal name",
              "enum": [
                "HUP",
                "INT",
                "QUIT",
                "ABRT",
                "KILL",
                "ALRM",
                "TERM",
                "USR1",
                "USR2",
                "URG",
                "CONT",
                "STOP",
                "TSTP",
                "TTIN",
                "TTOU"
              ]
            }
          }
